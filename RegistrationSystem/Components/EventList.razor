<div class="w-50 p-3" >
<table class="table table-borderless" width="100%" ti>
        <thead style="background-color: dodgerblue; text-align:center;" >
            <tr>
                <th colspan="5"><h2 style="color: white;">@title</h2></th>
            </tr>
    </thead>
    <tbody style="background-color: white;">
        @if (events != null)
        {
            @foreach (var eventOb in events)
            {
                    <EventListElement eventOb="@eventOb" OnEventDeleted="EventDeleted"></EventListElement>
                
            }
            @if (isFutureList)
                {
                    <tr>
                        <td colspan="5">
                            <NavLink class="nav-link" href="event/create">
                                <span class="oi oi-plus" aria-hidden="true"></span> LISA ÜRITUS
                            </NavLink>
                        </td>
                    </tr>
                }
        }
        else
        {
            <tr>
                <td><h3>No events available!</h3></td>
            </tr>
        }
    </tbody>
</table>
    
</div>
